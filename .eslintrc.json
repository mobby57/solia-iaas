{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/order": ["warn", { "alphabetize": { "order": "asc" } }]
  }
}
